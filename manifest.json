{
  "manifest_version": 2,
  "name": "Discussion Finder",
  "description": "Finds discussions for pages you view",
  "version": "0.2",
  "permissions": [
    "tabs",
    "http://www.reddit.com/",
    "http://api.thriftdb.com/"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
